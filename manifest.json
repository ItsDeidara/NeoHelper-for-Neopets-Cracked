{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "NeoHelper for Neopets - Cracked by StonedModder",
    "description": "Solution kit to improve your experience playing Neopets (Only works with Neopets in english).",
    "version": "1.0.7",
    "icons": {
      "128": "assets/img/128_active.png",
      "64": "assets/img/64_active.png"
    },
    "background": {
      "service_worker": "libs/background.js"
    },
    "content_scripts": [
      {
        "js": ["libs/content_script.js"],
        "css": [],
        "matches": ["https://www.neopets.com/*", "https://items.jellyneo.net/*"]
      }
    ],
    "action": {
      "default_title": "Click to NeoHelper settings",
      "default_popup": "index.html"
    },
    "permissions": [
      "externally_connectable",
      "storage",
      "cookies"
    ],
    "externally_connectable": {
      "matches": ["https://*.neopets.com/*", "https://items.jellyneo.net/*"]
    },
    "host_permissions": [
      "*://*.neopets.com/*",
      "https://api.telegram.org/*"
    ]
  }
  